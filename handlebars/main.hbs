 <div class="container">
        <table class="table ">
          <br>
          <br>
          <thead>
            <tr>
              <th scope="col">Nombre</th>
              <th scope="col">Precio</th>
              <th scope="col">Imagen</th>
            </tr>
          </thead>
          <tbody>
            <tr>{{#each data}}
              <td>{{this.title}}</td>
              <td>{{this.price}}</td>
              <td><img src={{this.thumbnail}} alt={{this.title}} width="100" height="100"/></td>
            </tr>
              {{/each}}
        </table>
      </div>